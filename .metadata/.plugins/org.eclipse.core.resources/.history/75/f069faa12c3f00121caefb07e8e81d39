import bb.cascades 1.0

SequentialAnimation {
    ScaleTransition {
        toX: 2.0
        toY: 2.0
        duration: 1000
    }
    ScaleTransition {
        toX: 1.5
        toY: 1.5
        duration: 500
    }
    ScaleTransition {
        toX: 1.0
        toY: 1.0
        duration: 3000
    }
    repeatCount: 5
}
