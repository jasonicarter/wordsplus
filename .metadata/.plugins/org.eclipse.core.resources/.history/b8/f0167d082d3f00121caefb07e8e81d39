import bb.cascades 1.0

SequentialAnimation {
    ScaleTransition {
        toX: 1.5
        toY: 1.5
        duration: 1000
    }
    ScaleTransition {
        toX: 1.0
        toY: 1.0
        duration: 1000
    }
    ScaleTransition {
        toX: 0.5
        toY: 0.5
        duration: 2000
    }
    repeatCount: 5
}
